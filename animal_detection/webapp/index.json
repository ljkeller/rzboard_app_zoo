{
    "name": "Animal Detection, USB",
    "description": "Detect and classify animals within a USB camera feed. Requires HDMI or MIPI display output",
    "process": {
        "command": "./animal_detection_usbcam_app",
        "cwd": "/home/root/model_zoo/animal_detection/exe",
        "args": [],
        "environment": [
            {
                "name": "XDG_SESSION_TYPE",
                "value": "tty"
            },
            {
                "name": "XDG_SESSION_CLASS",
                "value": "user"
            },
            {
                "name": "XDG_SESSION_ID",
                "value": "c2"
            },
            {
                "name": "XDG_RUNTIME_DIR",
                "value": "/run/user/0"
            }
        ]
    }
}