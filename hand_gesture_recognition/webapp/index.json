{
    "name": "Hand Gesture Recognition, USB",
    "description": "Recognize hand gestures within a USB camera feed. Integrated with Wayland- requires HDMI or MIPI display output",
    "process": {
        "command": "./hand_gesture_recognition_usbcam_app",
        "cwd": "/home/root/model_zoo/hand_gesture_recognition/exe",
        "args": [],
        "environment": [
            {
                "name": "XDG_SESSION_TYPE",
                "value": "tty"
            },
            {
                "name": "XDG_SESSION_CLASS",
                "value": "user"
            },
            {
                "name": "XDG_SESSION_ID",
                "value": "c2"
            },
            {
                "name": "XDG_RUNTIME_DIR",
                "value": "/run/user/0"
            }
        ]
    }
}