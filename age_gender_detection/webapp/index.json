{
    "name": "Age Gender Detection, USB",
    "description": "Predict age group and gender within a USB camera feed, requires HDMI or MIPI display output",
    "process": {
        "command": "./age_gender_classification_usbcam_app",
        "cwd": "/home/root/model_zoo/age_gender_detection/exe",
        "args": [],
        "environment": [
            {
                "name": "XDG_SESSION_TYPE",
                "value": "tty"
            },
            {
                "name": "XDG_SESSION_CLASS",
                "value": "user"
            },
            {
                "name": "XDG_SESSION_ID",
                "value": "c2"
            },
            {
                "name": "XDG_RUNTIME_DIR",
                "value": "/run/user/0"
            }
        ]
    }
}