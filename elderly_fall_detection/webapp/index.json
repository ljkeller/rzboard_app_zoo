{
    "name": "Elderly Fall Detection, USB",
    "description": "Detect falls within a USB camera feed, requires HDMI or MIPI display output",
    "process": {
        "command": "./elderly_fall_detection_usbcam_app",
        "cwd": "/home/root/model_zoo/elderly_fall_detection/exe",
        "args": [],
        "environment": [
            {
                "name": "XDG_SESSION_TYPE",
                "value": "tty"
            },
            {
                "name": "XDG_SESSION_CLASS",
                "value": "user"
            },
            {
                "name": "XDG_SESSION_ID",
                "value": "c2"
            },
            {
                "name": "XDG_RUNTIME_DIR",
                "value": "/run/user/0"
            }
        ]
    }
}