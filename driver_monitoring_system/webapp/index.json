{
    "name": "Driver Monitoring, USB",
    "description": "Describe attentiveness of a driver. Uses a USB camera feed, requires HDMI or MIPI display output",
    "process": {
        "command": "./driver_monitoring_system_usbcam_app",
        "cwd": "/home/root/model_zoo/driver_monitoring_system/exe",
        "args": [],
        "environment": [
            {
                "name": "XDG_SESSION_TYPE",
                "value": "tty"
            },
            {
                "name": "XDG_SESSION_CLASS",
                "value": "user"
            },
            {
                "name": "XDG_SESSION_ID",
                "value": "c2"
            },
            {
                "name": "XDG_RUNTIME_DIR",
                "value": "/run/user/0"
            }
        ]
    }
}